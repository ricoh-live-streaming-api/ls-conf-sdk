{
  "compilerOptions": {
    "target": "es2020",
    "lib": [],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "types": [
      "supertest",
      "jest"
    ]
  },
  "include": ["."]
}
